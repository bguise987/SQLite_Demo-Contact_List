<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ben.sqlitedemo_contactlist" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/contact_list_icon"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AddContact"
            android:label="@string/title_activity_add_contact" >
        </activity>
        <activity
            android:name=".DeleteContact"
            android:label="@string/title_activity_delete_contact" >
        </activity>
        <activity
            android:name=".QueryContact"
            android:label="@string/title_activity_query_contact" >
        </activity>
        <activity
            android:name=".add_contact"
            android:label="@string/title_activity_add_contact" >
        </activity>
        <activity
            android:name=".del_contact"
            android:label="@string/title_activity_del_contact" >
        </activity>
        <activity
            android:name=".query_contact"
            android:label="@string/title_activity_query_contact" >
        </activity>
    </application>

</manifest>
